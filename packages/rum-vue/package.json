{
  "name": "@rum/rum-vue",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/vue.umd.js",
  "module": "dist/vue.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue.esm.js",
      "require": "./dist/vue.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vue": "^3.4.0"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist || true",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "sideEffects": false
}
